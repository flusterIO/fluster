#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bigsexy/Desktop/fluster/packages/fluster_ts/node_modules/.pnpm/ts-protoc-gen@0.15.0/node_modules/ts-protoc-gen/bin/node_modules:/Users/bigsexy/Desktop/fluster/packages/fluster_ts/node_modules/.pnpm/ts-protoc-gen@0.15.0/node_modules/ts-protoc-gen/node_modules:/Users/bigsexy/Desktop/fluster/packages/fluster_ts/node_modules/.pnpm/ts-protoc-gen@0.15.0/node_modules:/Users/bigsexy/Desktop/fluster/packages/fluster_ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bigsexy/Desktop/fluster/packages/fluster_ts/node_modules/.pnpm/ts-protoc-gen@0.15.0/node_modules/ts-protoc-gen/bin/node_modules:/Users/bigsexy/Desktop/fluster/packages/fluster_ts/node_modules/.pnpm/ts-protoc-gen@0.15.0/node_modules/ts-protoc-gen/node_modules:/Users/bigsexy/Desktop/fluster/packages/fluster_ts/node_modules/.pnpm/ts-protoc-gen@0.15.0/node_modules:/Users/bigsexy/Desktop/fluster/packages/fluster_ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/protoc-gen-ts" "$@"
else
  exec node  "$basedir/../../bin/protoc-gen-ts" "$@"
fi
